#include <stdio.h>
int passedstduent(int m1,int m2,int m3)
{
    if(m1>=40&&m2>=40&&m3>=40){
    int s1,s2,s3;
    float avg;
    printf("enter the marks of s1,s2,s3\n");
    scanf("%d%d%d",&s1,&s2,&s3);
    avg=(s1+s2+s3)/3.0;
    printf("%f",avg);
    if (avg>=40){
        printf("pass\n");
    } else {
       printf("fail\n"); 
    }
    }  
}
int main(){
passedstduent (40,40,40);
}
