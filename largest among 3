#include<stdio.h>
    int largest(int a,int b,int c){
        if(a>=b&&a>=c){
            printf("a is large\n");
        }
       else if(b>=a&&b>=c){
            printf("b is large\n");
        }
        else{
        printf("c is large\n");
    }
    }
    int main(){
        largest(2,5,8);
    }
  
  
